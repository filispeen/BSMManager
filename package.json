{
  "name": "bsmmanager",
  "version": "1.3.2",
  "description": "Downloads maps directly into beat saber",
  "keywords": [
    "BS",
    "beat",
    "saber"
  ],
  "license": "ISC",
  "author": "FILISPEEN",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npx electron .",
    "build": "electron-builder",
    "pack": "electron-builder --dir",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "extract-zip": "^2.0.1",
    "fs": "^0.0.1-security",
    "p-limit": "^7.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.filispeen.bsmmanager",
    "productName": "BSMManager",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable",
        "zip"
      ]
    }
  }
